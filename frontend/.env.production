# Configuración para Frontend en Producción
# Copiar este archivo como .env.production en el directorio frontend/

# URL del API - en producción será relativa
REACT_APP_API_URL=/api

# URL del WebSocket - en producción será relativa
REACT_APP_WS_URL=ws://localhost/ws

# Configuración de la aplicación
REACT_APP_NAME=EMAS Weather System
REACT_APP_VERSION=1.0.0

# Configuración de build
GENERATE_SOURCEMAP=false
BUILD_PATH=build

# Configuración de analytics (opcional)
# REACT_APP_ANALYTICS_ID=

# Configuración de desarrollo (opcional)
# REACT_APP_DEBUG=false
